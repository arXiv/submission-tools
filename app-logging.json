{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "json": {
      "class": "tex2pdf.service.CustomJsonFormatter",
      "format": "%(asctime)s %(name)s %(levelname)s %(message)s %(process)d %(threadName)s"
    }
  },
  "handlers": {
    "access_handler": {
      "class": "logging.StreamHandler",
      "formatter": "json",
      "level": "INFO"
    }
  },
  "loggers": {
    "root": {
      "level": "INFO",
      "handlers": ["access_handler"],
      "propagate": true
    },
    "gunicorn": {
      "level": "INFO",
      "handlers": ["access_handler"],
      "propagate": false,
      "qualname": "gunicorn"
    },
    "uvicorn": {
      "level": "INFO",
      "handlers": ["access_handler"],
      "propagate": false,
      "qualname": "uvicorn"
    },
    "_granian": {
      "level": "INFO",
      "handlers": ["access_handler"],
      "propagate": false,
      "qualname": "granian"
    },
    "hypercorn": {
      "level": "INFO",
      "handlers": ["access_handler"],
      "propagate": false,
      "qualname": "hcorn"
    },
    "tex2pdf": {
      "level": "DEBUG",
      "handlers": ["access_handler"],
      "propagate": false,
      "qualname": "tex2pdf"
    }
  }
}
